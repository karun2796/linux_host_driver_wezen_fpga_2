PLATFORM?=WEZEN
FUNC?=WLAN_PWR
HACK_MONITOR_MODE?=0
KERNEL_VERSION?=5.15.0
INLINE_MODE?=Y
INLINE_MODE_RX?=N
BOUNCE_BUF?=N
RF?=B0
CMD_DEMO?=0
SECURE_DOMAIN?=Y
CMD_RX_BUFF?=N
ifeq ($(MODE), RADIO-TEST)
TWT_SUPPORT?=0
else
TWT_SUPPORT?=1
endif
LOW_POWER?=1
ifeq ($(PLATFORM),WEZEN)
MODE?=REG
ifeq ($(WLAN_SUPPORT), 1)
FW_LOAD?=ROM
endif


